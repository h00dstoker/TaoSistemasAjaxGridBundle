{
    "name": "taosistemas/ajaxgrid",
    "type": "symfony-bundle",
    "description": "Symfony AjaxGridBundle",
    "keywords": ["grid", "ajax"],
    "homepage": "https://github.com/omar331/TaoSistemasAjaxGridBundle",
    "license": "GPL 2.0",
    "authors": [
        {
            "name": "Omar Alves",
            "email": "omar331@gmail.com"
        },
        {
            "name": "Thiago Rodrigues",
            "email": "xthiago@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "carhartl/jquery-cookie": "1.4.0"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "~2.1"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": { "TaoSistemas\\AjaxGridBundle": "" }
    },
    "target-dir": "TaoSistemas/AjaxGridBundle",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "js-cookie/js-cookie",
                "version": "1.4.0",
                "source": {
                    "url": "git://github.com/js-cookie/js-cookie",
                    "type": "git",
                    "reference": "v1.4.0"
                }
            }
        }
    ]
}
